port './style.css'\nimport { wowInit } from '../wow';\nimport {\n    Carousel,\n    CarouselItem,\n    CarouselControl,\n    CarouselIndicators,\n} from 'reactstrap';\n\n\nexport default function News() {\n\n    useEffect(() => {\n        wowInit()\n    }, [])\n\n    const [activeIndex, setActiveIndex] = useState(0);\n    const [animating, setAnimating] = useState(false);\n\n    const next = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === news.length - 1 ? 0 : activeIndex + 1;\n        setActiveIndex(nextIndex);\n    }\n\n    const previous = () => {\n        if (animating) return;\n        const nextIndex = activeIndex === 0 ? news.length - 1 : activeIndex - 1;\n        setActiveIndex(nextIndex);\n    }\n\n    const goToIndex = (newIndex) => {\n        if (animating) return;\n        setActiveIndex(newIndex);\n    }\n\n    const newsItem = (news, index) => {\n        return (\n            <div className=\"news-item d-flex justify-content-center align-items-center flex-lg-column\" key={\"news\" + index}>\n                <div className=\"news-img\">\n                    <img src={news.img} alt=\"\" />\n                </div>\n                <div className=\"news-content\">\n                    <p className=\"text-center\">on {news.date}</p>\n                    <p className=\"news-title text-uppercase text-center\">\n                        {news.title}\n                    </p>\n                    <p className=\"text-center\">{news.subtitle} </p>\n                    <a href=\"#\">Đọc tiếp</a>\n                </div>\n\n            </div>\n        )\n    }\n\n    return (\n        <div className=\"news d-flex justify-content-center align-items-center flex-column wrapper\">\n            <img src={verticalLine} alt=\"\" className=\"vertical wow fadeInDown\" />\n            <p className=\"title text-center\">TIN TỨC</p>\n            <div className=\"news-container wow slideInLeft\">\n                {news.map((news, index) => newsItem(news, index))}\n            </div>\n            {/* news slide */}\n            <div className=\"news-slide-container\">\n                <Carousel\n                    activeIndex={activeIndex}\n                    next={next}\n                    previous={previous}\n                >\n                    <CarouselIndicators items={news} activeIndex={activeIndex} onClickHandler={goToIndex} />\n                    {news.map((news, index) => {\n                        return (\n                            <CarouselItem\n                                key={\"news\" + index}\n                                onExiting={() => setAnimating(true)}\n                                onExited={() => setAnimating(false)}>\n                                    {newsItem(news, index)}\n                            </CarouselItem>\n                        )\n                    })}\n                    <CarouselControl direction=\"prev\" directionText=\"Previous\" onClickHandler={previous} />\n                    <CarouselControl direction=\"next\" directionText=\"Next\" onClickHandler={next} />\n                </Carousel>\n            </div>\n            <img src={verticalLine} alt=\"\" className=\"vertical news-vertical-2  wow fadeInDown\" />\n        </div>\n    )\n}\n","/home/sonht/Downloads/react/demo-bytesoft-web-react/src/components/Service/index.js",["37"],{"ruleId":"38","severity":1,"message":"39","line":43,"column":7,"nodeType":"40","endLine":43,"endColumn":60},{"ruleId":"41","severity":1,"message":"42","line":24,"column":9,"nodeType":"40","endLine":24,"endColumn":46},{"ruleId":"41","severity":1,"message":"42","line":27,"column":9,"nodeType":"40","endLine":27,"endColumn":46},{"ruleId":"41","severity":1,"message":"42","line":30,"column":9,"nodeType":"40","endLine":30,"endColumn":46},{"ruleId":"41","severity":1,"message":"42","line":33,"column":9,"nodeType":"40","endLine":33,"endColumn":45},{"ruleId":"38","severity":1,"message":"39","line":42,"column":13,"nodeType":"4