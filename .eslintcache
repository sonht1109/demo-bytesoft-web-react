    const newsItem = (news, index) => {\n        return (\n            <div className=\"news-item d-flex justify-content-center align-items-center flex-lg-column\" key={\"news\" + index}>\n                <div className=\"news-img\">\n                    <img src={news.img} alt=\"\" />\n                </div>\n                <div className=\"news-content\">\n                    <p className=\"text-center\">on {news.date}</p>\n                    <p className=\"news-title text-uppercase text-center\">\n                        {news.title}\n                    </p>\n                    <p className=\"text-center\">{news.subtitle} </p>\n                    <a href=\"#\">Đọc tiếp</a>\n                </div>\n\n            </div>\n        )\n    }\n\n    return (\n        <div className=\"news d-flex justify-content-center align-items-center flex-column wrapper\">\n            <img src={verticalLine} alt=\"\" className=\"vertical wow fadeInDown\" />\n            <p className=\"title text-center\">TIN TỨC</p>\n            <div className=\"news-container wow slideInLeft\">\n                {news.map((news, index) => newsItem(news, index))}\n            </div>\n            {/* news slide */}\n            <div className=\"news-slide-container\">\n                <Carousel\n                    activeIndex={activeIndex}\n                    next={next}\n                    previous={previous}\n                >\n                    <CarouselIndicators items={news} activeIndex={activeIndex} onClickHandler={goToIndex} />\n                    {news.map((news, index) => {\n                        return (\n                            <CarouselItem\n                                key={\"news\" + index}\n                                onExiting={() => setAnimating(true)}\n                                onExited={() => setAnimating(false)}>\n                                    {newsItem(news, index)}\n                            </CarouselItem>\n                        )\n                    })}\n                    <CarouselControl direction=\"prev\" directionText=\"Previous\" onClickHandler={previous} />\n                    <CarouselControl direction=\"next\" directionText=\"Next\" onClickHandler={next} />\n                </Carousel>\n            </div>\n            <img src={verticalLine} alt=\"\" className=\"vertical news-vertical-2  wow fadeInDown\" />\n        </div>\n    )\n}\n","/home/sonht/Downloads/react/demo-bytesoft-web-react/src/components/Service/index.js",["55"],"import React, { useEffect, useState } from 'react'\nimport { services } from './services'\nimport { Container, Row, Col } from 'reactstrap'\nimport './style.css'\nimport { wowInit } from '../wow'\n\nexport default function Service() {\n\n  const [index, setIndex] = useState(0)\n\n  useEffect(() => {\n    wowInit()\n  }, [])\n\n  const rightBlockItem = (item) => {\n    return (\n      <div className=\"right-block-wrapper\">\n        <p>{item.name}</p>\n        <span style={{ lineHeight: \"24px\" }}>{item.content[0]}</span>\n        <div className=\"service-detail\">\n          <span style={{ lineHeight: \"24px\" }}>{item.content[1]}</span>\n          <img src={item.contentImg} alt=\"\" style={{ marginLeft: 20 }} />\n        </div>\n        <span style={{ lineHeight: \"24px\" }}>{item.content[2]}</span>\n        <br />\n        <a href=\"#\" className=\"read-more\">XEM THÊM</a>\n      </div>\n    )\n  }\n\n  const leftBlockItem = (item, i) => {\n    return (\n      <div\n        className={`block-item ${index === i && 'active'}`}\n        onClick={() => setIndex(i)}\n        key={\"leftBlock\" + i}>\n        <div className=\"block-img\">\n          <img src={item.inactiveImg} alt=\"\" />\n          <img src={item.activeImg} alt=\"\" />\n        </div>\n        <span>{item.name}</span>\n      </div>\n    )\n  }\n\n  return (\n    <div className=\"service wrapper\">\n      <div className=\"service-content\">\n        <div className=\"top-content\">\n          <div className=\"title\">DỊCH VỤ - BYTESOFT</div>\n          <span className=\"wow fadeInDown\">Với tiêu chí \"Chất lượng hơn số lượng. ByteSoft luôn tạo ra những sản phẩm có giá\n          trị\n          cao nhất\". Đến với\n              chúng tôi, khách hàng sẽ luôn được cung cấp những dịch vụ tốt nhất</span>\n        </div>\n        <div className=\"service-block\">\n          <Container className=\"d-flex justify-content-center align-items-center p-0\">\n            <Row style={{width: \"100%\"}}>\n              <Col xl=\"5\" className=\"left-block align-self-center wow fadeInLeft\">\n                {\n                  services.map((item, index) => {\n                    return leftBlockItem(item, index)\n                  })\n                }\n              </Col>\n\n              <Col xl=\"7\" className=\"right-block wow fadeInDown\">\n                {rightBlockItem(services[index])}\n              </Col>\n            </Row>\n          </Container>\n        </div>\n      </div>\n    </div>\n  )\n}\n",["56","57"],"/home/sonht/Downloads/react/demo-bytesoft-web-react/src/reportWebVitals.js",[],"/home/sonht/Downloads/react/demo-bytesoft-web-react/src/components/Footer/index.js",["58","59","60","61","62","63","64","65"],"/home/sonht/Downloads/react/demo-bytesoft-web-react/src/components/Hiring/hirings.js",[],"/home/sonht/Downloads/react/demo-bytesoft-web-react/src/components/Service/services.js",[],{"ruleId":"66","severity":1,"message":"67","line":43,"column":7,"nodeType":"68","endLine":43,"endColumn":60},{"ruleId":"69","severity":1,"message":"70","line":24,"column":9,"nodeType":"68","endLine":24,"endColumn":46},{"ruleId":"69","severity":1,"message":"70","line":27,"column":9,"nodeType":"68","endLine":27,"endColumn":46},{"ruleId":"69","severity":1,"message":"70","line":30,"column":9,"nodeType":"68","endLine":30,"endColumn":46},{"ruleId":"69","severity":1,"message":"70","line":33,"column":9,"nodeType":"68","endLine":33,"endColumn":45},{"ruleId":"66","severity":1,"message